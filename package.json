{
  "name": "botium-cli",
  "version": "0.0.51",
  "description": "Botium - The Selenium for Chatbots",
  "main": "dist/botium-cli-cjs.js",
  "module": "dist/botium-cli-es.js",
  "engines": {
    "node": ">=8.3"
  },
  "bin": {
    "botium-cli": "./bin/botium-cli.js"
  },
  "scripts": {
    "build": "npm run eslint && rollup -c",
    "eslint": "eslint \"./src/**/*.js\" \"./bin/**/*.js\"",
    "buildbin": "./node_modules/.bin/pkg --targets node10-linux-x64,node10-linux-x86,node10-win-x64,node10-win-x86,node10-macos-x64,node10-macos-x86 --out-path dist/bin ./package.json",
    "newpatch": "npm version patch"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "bin/**/*",
      "src/emulator/browser/public/**/*",
      "src/emulator/browser/views/**/*",
      "node_modules/figlet/fonts/*",
      "node_modules/botium-core",
      "node_modules/botium-connector-alexa-avs",
      "node_modules/botium-connector-alexa-smapi",
      "node_modules/botium-connector-botpress",
      "node_modules/botium-connector-botkit",
      "node_modules/botium-connector-botkit-websocket",
      "node_modules/botium-connector-directline3",
      "node_modules/botium-connector-dialogflow",
      "node_modules/botium-connector-echo",
      "node_modules/botium-connector-fbpagereceiver",
      "node_modules/botium-connector-google-assistant",
      "node_modules/botium-connector-koreai-webhook",
      "node_modules/botium-connector-lex",
      "node_modules/botium-connector-luis",
      "node_modules/botium-connector-rasa",
      "node_modules/botium-connector-watson",
      "node_modules/botium-connector-webdriverio",
      "node_modules/botium-connector-wechat",
      "node_modules/opn",
      "node_modules/esprima",
      "*.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeforequity-at/botium-cli.git"
  },
  "author": "Florian Treml",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeforequity-at/botium-cli/issues"
  },
  "homepage": "https://github.com/codeforequity-at/botium-cli#readme",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "license-checker": "^25.0.1",
    "pkg": "^4.4.0",
    "rollup": "^1.15.6",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.3"
  },
  "dependencies": {
    "async": "^3.0.1",
    "body-parser": "^1.19.0",
    "botium-connector-alexa-avs": "0.0.5",
    "botium-connector-alexa-smapi": "0.0.6",
    "botium-connector-botkit": "0.0.3",
    "botium-connector-botkit-websocket": "0.1.1",
    "botium-connector-botpress": "0.0.4",
    "botium-connector-dialogflow": "0.0.8",
    "botium-connector-directline3": "0.0.14",
    "botium-connector-echo": "0.0.1",
    "botium-connector-fbpagereceiver": "0.0.2",
    "botium-connector-google-assistant": "0.0.2",
    "botium-connector-koreai-webhook": "0.0.2",
    "botium-connector-lex": "0.0.2",
    "botium-connector-luis": "0.0.1",
    "botium-connector-rasa": "0.0.1",
    "botium-connector-watson": "0.0.7",
    "botium-connector-webdriverio": "0.1.1",
    "botium-connector-wechat": "0.0.1",
    "botium-core": "1.5.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "ejs": "^2.6.2",
    "esprima": "git+https://github.com/codeforequity-at/jsonpath.git",
    "express": "^4.17.1",
    "figlet": "^1.2.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "mochawesome": "^4.0.1",
    "opn": "^6.0.0",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "slug": "^1.1.0",
    "socket.io": "^2.2.0",
    "terminal-kit": "^1.28.1",
    "yargs": "^13.2.4"
  }
}
